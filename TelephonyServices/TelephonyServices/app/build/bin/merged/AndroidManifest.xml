<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.telephonyservices"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/callmessage"
        android:label="@string/app_name"
        android:resizeableActivity="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.telephonyservices.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.telephonyservices.Message1Activity" >
        </activity>
        <activity android:name="com.telephonyservices.Message2Activity" >
        </activity>
        <activity android:name="com.telephonyservices.Message3Activity" >
        </activity>
        <activity android:name="com.telephonyservices.Message4Activity" >
        </activity>
        <activity android:name="com.telephonyservices.Message5Activity" >
        </activity>
    </application>

</manifest>